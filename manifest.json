{
    "manifest_version": 3,
    "name": "SEO Markup Viewer",
    "version": "1.0",
    "description": "View SEO-relevant HTML markup on any webpage",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }